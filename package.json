{
  "name": "stratonea-frontend-starter-v1",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm:dev:vite\" \"npm:dev:typecheck\"",
    "dev:vite": "vite",
    "dev:typecheck": "vue-tsc -b --noEmit --watch --pretty",
    "build": "npm run generate-icons && vue-tsc -b && vite build",
    "preview": "vite preview",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "dev:slow": "VITE_NETWORK=slow vite",
    "analyze": "vite build --mode analyze",
    "lighthouse": "lighthouse http://localhost:4173 --view --preset=mobile",
    "size-check": "npm run build && bundlesize",
    "network-test": "node scripts/network-test.js",
    "generate-icons": "node scripts/generate-icons.js"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.7.2",
    "@fortawesome/free-brands-svg-icons": "^6.7.2",
    "@fortawesome/free-regular-svg-icons": "^6.7.2",
    "@fortawesome/free-solid-svg-icons": "^6.7.2",
    "@fortawesome/vue-fontawesome": "^3.0.8",
    "@headlessui/vue": "^1.7.23",
    "@vercel/analytics": "^1.5.0",
    "@vueuse/core": "^13.1.0",
    "axios-retry": "^4.5.0",
    "html2pdf.js": "^0.10.3",
    "localforage": "^1.10.0",
    "pinia": "^3.0.2",
    "pinia-plugin-persistedstate": "^4.3.0",
    "sharp": "^0.34.1",
    "vue-router": "^4.5.1",
    "vue-toastification": "^2.0.0-rc.5",
    "workbox-recipes": "^7.3.0",
    "zod": "^3.24.4"
  },
  "devDependencies": {
    "@tailwindcss/forms": "^0.5.10",
    "@testing-library/vue": "^8.1.0",
    "@types/lodash": "^4.17.16",
    "@types/node": "^22.15.17",
    "@types/testing-library__vue": "^5.3.0",
    "@vitejs/plugin-vue": "^5.0.0",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.7.0",
    "autoprefixer": "^10.4.17",
    "concurrently": "^9.1.2",
    "postcss": "^8.4.35",
    "postcss-import": "^16.1.0",
    "tailwindcss": "^3.4.1",
    "vite": "^6.3.5",
    "vite-plugin-pwa": "^1.0.0",
    "vitest": "^3.1.3",
    "vue": "^3.5.14",
    "vue-eslint-parser": "^10.1.3",
    "vue-tsc": "^2.2.8",
    "web-vitals": "^5.0.0",
    "workbox-build": "^7.3.0",
    "workbox-window": "^7.3.0"
  }
}
